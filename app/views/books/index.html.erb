<%= render 'users/index', users:@user %>

<h2>Book detail</h2>
<p>Title</p>
<p>Body</p>
<% @books.each do |book| %>
 <tr>
<td><%= book.title %></td>
<td><%= book.body %></td>
<td><%= link_to "Edit", edit_book_path(book.id)  %></td>
<td><%= link_to 'Destroy', books_path, method: :DELETE, "data-confirm" => "本当に消しますか?" %></td>
<% end %>